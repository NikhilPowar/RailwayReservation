<!-- This page shows summary of tickets -->
<html>
  <head>
    <title>View Tickets</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <!-- Jquery library -->
    <script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.5.min.js" type="text/javascript"></script>
    <!-- Bootstrap JS -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <link rel="stylesheet" type="text/css" href="pagelayout.css">
  </head>
  <body>
    <% include header.ejs %>
    <% include sidebar.ejs %>
    <div class='container'>
    <h4>
    Your booking is confirmed.<br>
    Present a printed copy of this acknowledgement to receive your ticket while boarding.<br><br>
    Train name: <%= session.ticket.selected.train_name %><br>
    Train no. : <%= session.ticket.selected.train_num %><br>
    Date: <%= session.ticket.selected.date %><br>
    From: <%= session.ticket.selected.from %><br>
    To: <%= session.ticket.selected.to %><br>
    Class: <%= session.ticket.selected.classtype %><br>
    Seats: <%= session.ticket.selected.seats %><br>
    Total fare: <%= session.ticket.selected.total_fare %>
    </h4>
    </div>
    <% include footer.ejs %>
  </body>
</html>
