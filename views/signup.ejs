<!-- This is the page rendered for signup -->
<html>
   <body>
      <form action="/signup-controller" method="post" enctype="application/x-www-form-urlencoded">
         <fieldset>
            <label for="fullname">Full Name:</label>
            <input type="text" id="fullname" name="fullname" placeholder="Enter your full name" />
            <br>
            <label for="email">Email:</label>
            <input type="email" id="email" name="email" placeholder="Enter your email address" />
            <br>
            <label for="password">Password:</label>
            <input type="password" id="password" name="password" placeholder="Enter password"></input>
            <br>
            <label for="confirm">Confirm Password:</label>
            <input type="password" id="confirm" name="confirm" placeholder="Confirm password"></input>
            <br>
            <input type="submit" value="Create Account"/>
         </fieldset>
      </form>
   </body>
</html>
